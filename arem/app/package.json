    "envalid": "^8.1.0",
    "grammy": "^1.38.3",
    "hono": "^4.10.4",
    "hono-rate-limiter": "^0.4.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "moodle-api": "^0.1.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "tough-cookie": "^6.0.0",
    "turndown": "^7.2.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
