{
  "name": "@remoodle/app",
  "version": "2.7.4",
  "type": "module",
  "private": true,
  "exports": {
    "./api": {
      "types": "./src/services/api/index.ts"
    }
  },
  "scripts": {
    "dev:api": "bun --watch src/services/api/index.ts",
    "dev:cluster": "bun --watch src/services/cluster/index.ts",
    "dev:bot": "bun --watch src/services/bot/index.ts",
    "dev": "run-p dev:*",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "test": "TZ=UTC vitest"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.0",
